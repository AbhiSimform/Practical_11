@model Practical_11_Test2.Models.User

<tr>
	<td>
		@Model.Name
	</td>
	<td>
		@Model.DOB
	</td>
	<td>
		@Model.Address
	</td>
	<td>
		@Html.ActionLink("Details", "Detail", new { id = Model.Id }, htmlAttributes: new { @class = "btn btn-success btn-sm" })
		@Html.ActionLink("Edit", "Update", new { id = Model.Id }, htmlAttributes: new { @class = "btn btn-success btn-sm" })
		@Html.ActionLink("Delete", "Delete", new { id = Model.Id }, htmlAttributes: new { @class = "btn btn-danger btn-sm" })

	</td>
</tr>
